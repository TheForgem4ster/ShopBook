@using DataLayer.Models

<h1>Find</h1>


<div>
    @{
        foreach (Book item in Model)
        {
            <img width="300" height="450" src="@Url.Content(item.img)" alt="" /><br>
            @item.Name<br>
            @item.shortDesc<br>
            @item.grade<br>
            @item.price<br>
            
            //@Html.DisplayFor(modelItem => item.img)
            //@Html.Partial("FindBooks", item)
            
        }
    }
</div>